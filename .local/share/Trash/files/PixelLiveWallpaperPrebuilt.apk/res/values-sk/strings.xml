    <string name="imprint_wallpaper_name">Záblesk</string>
    <string name="kent_wallpaper_description">Podľa rozsiahlych ovocných sadov a chmeľových polí sa grófstvu Kent hovorí „Záhrada Anglicka“.</string>
    <string name="kent_wallpaper_name">Záhrada, Kent, Spojené kráľovstvo</string>
    <string name="lagos_wallpaper_description">Pozorujte naživo tyrkysové vlny narážajúce na Ponta da Piedade, skupinu skalných formácií pozdĺž pobrežia Atlantiku. Lagos, Portugalsko</string>
